                                <jaxb:globalBindings/>
                        </xsd:appinfo>
                        <jaxb:schemaBindings>
                                <jaxb:package name="org.docx4j.vml.officedrawing"/>
                        </jaxb:schemaBindings>
                </xsd:appinfo>
        </xsd:annotation>
  
  
  <xsd:attribute name="bwmode" type="ST_BWMode" />
