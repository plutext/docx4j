    <xsd:attributeGroup ref="v:AG_Ext" />
    <xsd:attribute name="issignatureline" type="ST_TrueFalse">
      <xsd:annotation>
        <xsd:documentation>Signature Line Flag</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
