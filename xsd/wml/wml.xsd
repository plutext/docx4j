				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="accent4">
				<xsd:annotation>
					<xsd:documentation>Accent4 Theme Color
						Reference</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="accent5">
				<xsd:annotation>
					<xsd:documentation>Accent5 Theme Color
						Reference</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="accent6">
				<xsd:annotation>
					<xsd:documentation>Accent 6 Theme Color
						Reference</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="hyperlink">
				<xsd:annotation>
					<xsd:documentation>Hyperlink Theme Color
						Reference</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="followedHyperlink">
				<xsd:annotation>
					<xsd:documentation>Followed Hyperlink Theme Color
						Reference</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="CT_ColorSchemeMapping">
