			<xsd:attribute name="rsidRPr" type="ST_LongHexNumber">
				<xsd:annotation>
					<xsd:documentation>Revision Identifier for Table Row Glyph
						Formatting</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="rsidR" type="ST_LongHexNumber">
				<xsd:annotation>
