					</xsd:attribute>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="tblStyleColBandSize" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Number of Columns in Column
						Band</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="val" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:integer"/>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="tblW" type="CT_TblWidth" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Preferred Table Width</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="jc" type="CT_Jc" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Table Alignment</xsd:documentation>
				</xsd:annotation>
