			<xsd:attribute name="rsidDel" type="ST_LongHexNumber">
				<xsd:annotation>
					<xsd:documentation>Revision Identifier for Table Row
						Deletion</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="rsidTr" type="ST_LongHexNumber">
				<xsd:annotation>
					<xsd:documentation>Revision Identifier for Table Row
