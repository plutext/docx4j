					</xsd:annotation>
				</xsd:element>
				<xsd:element name="picture">
					<xsd:annotation>
						<xsd:documentation>Picture Structured Document
							Tag</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType/>
				</xsd:element>
				<xsd:element name="richText">
					<xsd:annotation>
						<xsd:documentation>Rich Text Structured Document
							Tag</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType/>
				</xsd:element>
				<xsd:element name="text" type="CT_SdtText">
					<xsd:annotation>
						<xsd:documentation>Plain Text Structured Document
							Tag</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="citation">
					<xsd:annotation>
						<xsd:documentation>Citation Structured Document
							Tag</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType/>
				</xsd:element>
				<xsd:element name="group">
					<xsd:annotation>
						<xsd:documentation>Group Structured Document
							Tag</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType/>
				</xsd:element>
				<xsd:element name="bibliography">
					<xsd:annotation>
						<xsd:documentation>Bibliography Structured Document
							Tag</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType/>
				</xsd:element>
			</xsd:choice>
			<xsd:element ref="w14:checkbox"  minOccurs="0"/>
			<xsd:element ref="w14:entityPicker"  minOccurs="0"/>
			<xsd:element ref="w15:appearance"  minOccurs="0"/>
			<xsd:element ref="w15:color"  minOccurs="0"/>
			<xsd:element ref="w15:dataBinding"  minOccurs="0"/>
			<xsd:element ref="w15:repeatingSection"  minOccurs="0"/>
			<xsd:element ref="w15:repeatingSectionItem"  minOccurs="0"/>
			<xsd:element ref="w15:webExtensionCreated"  minOccurs="0"/>
			<xsd:element ref="w15:webExtensionLinked"  minOccurs="0"/>
