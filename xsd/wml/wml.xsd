	<xsd:complexType name="CT_ReadingModeInkLockDown">
		<xsd:attribute name="actualPg" type="xsd:boolean" use="required">
			<xsd:annotation>
				<xsd:documentation>Use Actual Pages, Not Virtual
					Pages</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="w" type="ST_PixelsMeasure" use="required">
			<xsd:annotation>
				<xsd:documentation>Virtual Page Width</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="h" type="ST_PixelsMeasure" use="required">
			<xsd:annotation>
				<xsd:documentation>Virtual Page Height</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="fontSz" type="ST_DecimalNumber" use="required">
			<xsd:annotation>
				<xsd:documentation>Font Size Scaling</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="CT_WriteProtection">
		<xsd:attribute name="recommended" type="xsd:boolean" default="true"
			use="optional">
			<xsd:annotation>
				<xsd:documentation>Recommend Write Protection in User
					Interface</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attributeGroup ref="AG_Password"></xsd:attributeGroup>
	</xsd:complexType>
	<xsd:complexType name="CT_Settings">
		<xsd:sequence>
			<xsd:element name="writeProtection" type="CT_WriteProtection"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Write Protection</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="view" type="CT_View" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Document View Setting</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="zoom" type="CT_Zoom" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Magnification Setting</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="removePersonalInformation"
				type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Remove Personal Information from Document
						Properties</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="removeDateAndTime" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Remove Date and Time from
						Annotations</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doNotDisplayPageBoundaries"
				type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Display Visual Boundary For
						Header/Footer or Between Pages</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="displayBackgroundShape" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Display Background Objects When Displaying
						Document</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="printPostScriptOverText" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Print PostScript Codes With Document
						Text</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="printFractionalCharacterWidth"
				type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Print Fractional Character
						Widths</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="printFormsData" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Only Print Form Field
						Content</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="embedTrueTypeFonts" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Embed TrueType Fonts</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="embedSystemFonts" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Embed Common System
						Fonts</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="saveSubsetFonts" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Subset Fonts When
						Embedding</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="saveFormsData" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Only Save Form Field
						Content</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="mirrorMargins" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Mirror Page Margins</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="alignBordersAndEdges" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Align Paragraph and Table Borders with Page
						Border</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="bordersDoNotSurroundHeader"
				type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Page Border Excludes
						Header</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="bordersDoNotSurroundFooter"
				type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Page Border Excludes
						Footer</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="gutterAtTop" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Position Gutter At Top of
						Page</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="hideSpellingErrors" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Display Visual Indication of Spelling
						Errors</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="hideGrammaticalErrors" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Display Visual Indication of Grammatical
						Errors</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="activeWritingStyle" type="CT_WritingStyle"
				minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Grammar Checking
						Settings</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="proofState" type="CT_Proof" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Spelling and Grammatical Checking
						State</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="formsDesign" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Structured Document Tag Placeholder Text
						Should be Resaved</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="attachedTemplate" type="CT_Rel" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Attached Document
						Template</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="linkStyles" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Automatically Update Styles From Document
						Template</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="stylePaneFormatFilter" type="CT_ShortHexNumber"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Suggested Filtering for List of Document
						Styles</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="stylePaneSortMethod" type="CT_ShortHexNumber"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Suggested Sorting for List of Document
						Styles</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="documentType" type="CT_DocType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Document
						Classification</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="mailMerge" type="CT_MailMerge" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Mail Merge Settings</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="revisionView" type="CT_TrackChangesView"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Visibility of Annotation
						Types</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="trackRevisions" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Track Revisions to
						Document</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doNotTrackMoves" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Use Move Syntax When Tracking
						Revisions</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doNotTrackFormatting" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Track Formatting Revisions When Tracking
						Revisions</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="documentProtection" type="CT_DocProtect"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Document Editing
						Restrictions</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="autoFormatOverride" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Allow Automatic Formatting to Override
						Formatting Protection Settings</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="styleLockTheme" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Prevent Modification of Themes
						Part</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="styleLockQFSet" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Prevent Replacement of Styles
						Part</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="defaultTabStop" type="CT_TwipsMeasure"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Distance Between Automatic Tab
						Stops</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="autoHyphenation" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Automatically Hyphenate Document Contents
						When Displayed</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="consecutiveHyphenLimit" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Maximum Number of Consecutively Hyphenated
						Lines</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="val" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:integer"/>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="hyphenationZone" type="CT_TwipsMeasure"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Hyphenation Zone</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doNotHyphenateCaps" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Hyphenate Words in ALL CAPITAL
						LETTERS</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="showEnvelope" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Show E-Mail Message
						Header</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="summaryLength" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Percentage of Document to Use When Generating
						Summary</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="val" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:integer"/>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="clickAndTypeStyle" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Paragraph Style Applied to Automatically
						Generated Paragraphs</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="val" type="xsd:string"/>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="defaultTableStyle" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Default Table Style for Newly Inserted
						Tables</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="val" type="xsd:string"/>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="evenAndOddHeaders" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Different Even/Odd Page Headers and
						Footers</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="bookFoldRevPrinting" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Reverse Book Fold
						Printing</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="bookFoldPrinting" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Book Fold Printing</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="bookFoldPrintingSheets" minOccurs="0">
