				<xsd:annotation>
					<xsd:documentation>Table Borders</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="shd" type="CT_Shd" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Table Shading</xsd:documentation>
				</xsd:annotation>
