				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="dataSource" type="CT_Rel" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Data Source File Path</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="headerSource" type="CT_Rel" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Header Definition File
						Path</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doNotSuppressBlankLines" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Remove Blank Lines from Merged
						Documents</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="destination" type="CT_MailMergeDest" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Merged Document
						Destination</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="addressFieldName" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Column Containing E-mail
						Address</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="val" type="xsd:string"/>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="mailSubject" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Merged E-mail or Fax Subject
						Line</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="val" type="xsd:string"/>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="mailAsAttachment" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Merged Document To E-Mail
						Attachment</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="viewMergedData" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>View Merged Data Within
						Document</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="activeRecord" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Record Currently Displayed In Merged
						Document</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="val" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:integer"/>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="checkErrors" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Mail Merge Error Reporting
						Setting</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="val" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:integer"/>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="odso" type="CT_Odso" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Office Data Source Object
						Settings</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="ST_TargetScreenSz">
		<xsd:annotation>
			<xsd:documentation>Target Screen Sizes for Generated Web
				Pages</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="544x376">
				<xsd:annotation>
					<xsd:documentation>Optimize for 544x376</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="640x480">
				<xsd:annotation>
					<xsd:documentation>Optimize for 640x480</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="720x512">
				<xsd:annotation>
					<xsd:documentation>Optimize for 720x512</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="800x600">
				<xsd:annotation>
					<xsd:documentation>Optimize for 800x600</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="1024x768">
				<xsd:annotation>
					<xsd:documentation>Optimize for 1024x768</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="1152x882">
				<xsd:annotation>
					<xsd:documentation>Optimize for 1152x882</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="1152x900">
				<xsd:annotation>
					<xsd:documentation>Optimize for 1152x900</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="1280x1024">
				<xsd:annotation>
					<xsd:documentation>Optimize for 1280x1024</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="1600x1200">
				<xsd:annotation>
					<xsd:documentation>Optimize for 1600x1200</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="1800x1440">
				<xsd:annotation>
					<xsd:documentation>Optimize for 1800x1440</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="1920x1200">
				<xsd:annotation>
					<xsd:documentation>Optimize for 1920x1200</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="CT_TargetScreenSz">
		<xsd:attribute name="val" type="ST_TargetScreenSz" use="required">
			<xsd:annotation>
				<xsd:documentation>Target Screen Size Value</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="CT_Compat">
		<xsd:sequence>
			<xsd:element name="useSingleBorderforContiguousCells"
				type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Use Simplified Rules For Table Border
						Conflicts</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="wpJustification" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Emulate WordPerfect 6.x Paragraph
						Justification</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="noTabHangInd" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Create Custom Tab Stop for Hanging
						Indent</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="noLeading" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Add Leading Between Lines of
						Text</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="spaceForUL" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Add Additional Space Below Baseline For
						Underlined East Asian Text</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="noColumnBalance" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Balance Text Columns within a
						Section</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="balanceSingleByteDoubleByteWidth"
				type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Balance Single Byte and Double Byte
						Characters</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="noExtraLineSpacing" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Center Content on Lines With Exact Line
						Height</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doNotLeaveBackslashAlone" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Convert Backslash To Yen Sign When
						Entered</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ulTrailSpace" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Underline All Trailing
						Spaces</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doNotExpandShiftReturn" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Don't Justify Lines Ending in Soft Line
						Break</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="spacingInWholePoints" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Only Expand/Condense Text By Whole
						Points</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="lineWrapLikeWord6" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Emulate Word 6.0 Line Wrapping for East Asian
						Text</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="printBodyTextBeforeHeader"
				type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Print Body Text before Header/Footer
						Contents</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="printColBlack" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Print Colors as Black And White without
						Dithering</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="wpSpaceWidth" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Space width</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="showBreaksInFrames" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Display Page/Column Breaks Present in
						Frames</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="subFontBySize" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Increase Priority Of Font Size During Font
						Substitution</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="suppressBottomSpacing" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Ignore Exact Line Height for Last Line on
						Page</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="suppressTopSpacing" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Ignore Minimum and Exact Line Height for First
						Line on Page</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="suppressSpacingAtTopOfPage"
				type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Ignore Minimum Line Height for First Line on
						Page</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="suppressTopSpacingWP" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Emulate WordPerfect 5.x Line
						Spacing</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="suppressSpBfAfterPgBrk" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Use Space Before On First Line After a Page
						Break</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="swapBordersFacingPages" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Swap Paragraph Borders on Odd Numbered
						Pages</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="convMailMergeEsc" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Treat Backslash Quotation Delimiter as Two
						Quotation Marks</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="truncateFontHeightsLikeWP6"
				type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Emulate WordPerfect 6.x Font Height
						Calculation</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="mwSmallCaps" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Emulate Word 5.x for the Macintosh Small Caps
						Formatting</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="usePrinterMetrics" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Use Printer Metrics To Display
						Documents</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doNotSuppressParagraphBorders"
				type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Suppress Paragraph Borders Next To
						Frames</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="wrapTrailSpaces" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Line Wrap Trailing
						Spaces</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="footnoteLayoutLikeWW8" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Emulate Word 6.x/95/97 Footnote
						Placement</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="shapeLayoutLikeWW8" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Emulate Word 97 Text Wrapping Around Floating
						Objects</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="alignTablesRowByRow" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Align Table Rows
						Independently</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="forgetLastTabAlignment" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Ignore Width of Last Tab Stop When Aligning
						Paragraph If It Is Not Left Aligned</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="adjustLineHeightInTable" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Add Document Grid Line Pitch To Lines in Table
						Cells</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="autoSpaceLikeWord95" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Emulate Word 95 Full-Width Character
						Spacing</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="noSpaceRaiseLower" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Increase Line Height for Raised/Lowered
						Text</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doNotUseHTMLParagraphAutoSpacing"
				type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Use Fixed Paragraph Spacing for HTML Auto
						Setting</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="layoutRawTableWidth" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Ignore Space Before Table When Deciding If Table
						Should Wrap Floating Object</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="layoutTableRowsApart" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Allow Table Rows to Wrap Inline Objects
						Independently</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="useWord97LineBreakRules" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Emulate Word 97 East Asian Line
						Breaking</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doNotBreakWrappedTables" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Allow Floating Tables To Break Across
						Pages</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doNotSnapToGridInCell" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Snap to Document Grid in Table Cells with
						Objects</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="selectFldWithFirstOrLastChar"
				type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Select Field When First or Last Character Is
						Selected</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="applyBreakingRules" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Use Legacy Ethiopic and Amharic Line Breaking
						Rules</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doNotWrapTextWithPunct" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Allow Hanging Punctuation With Character
						Grid</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doNotUseEastAsianBreakRules"
				type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Compress Compressible Characters When
						Using Document Grid</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="useWord2002TableStyleRules"
				type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Emulate Word 2002 Table Style
						Rules</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="growAutofit" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Allow Tables to AutoFit Into Page
						Margins</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="useFELayout" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Bypass East Asian/Complex Script Layout
						Code</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="useNormalStyleForList" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Automatically Apply List Paragraph Style
						To Bulleted/Numbered Text</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doNotUseIndentAsNumberingTabStop"
				type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Ignore Hanging Indent When Creating Tab Stop
						After Numbering</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="useAltKinsokuLineBreakRules"
				type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Use Alternate Set of East Asian Line Breaking
						Rules</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="allowSpaceOfSameStyleInTable"
				type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Allow Contextual Spacing of Paragraphs in
						Tables</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doNotSuppressIndentation" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Ignore Floating Objects When Calculating
						Paragraph Indentation</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doNotAutofitConstrainedTables"
				type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not AutoFit Tables To Fit Next To Wrapped
						Objects</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="autofitToFirstFixedWidthCell"
				type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Allow Table Columns To Exceed Preferred Widths
						of Constituent Cells</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="underlineTabInNumList" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Underline Following Character Following
						Numbering</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="displayHangulFixedWidth" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Always Use Fixed Width for Hangul
						Characters</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="splitPgBreakAndParaMark" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Always Move Paragraph Mark to Page after a Page
						Break</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doNotVertAlignCellWithSp" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Don't Vertically Align Cells Containing
						Floating Objects</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doNotBreakConstrainedForcedTable"
				type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Don't Break Table Rows Around Floating
						Tables</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doNotVertAlignInTxbx" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Ignore Vertical Alignment in
						Textboxes</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="useAnsiKerningPairs" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Use ANSI Kerning Pairs from
						Fonts</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="cachedColBalance" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Use Cached Paragraph Information for Column
						Balancing</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="compatSetting" type="CT_CompatSetting" minOccurs="0" maxOccurs="unbounded">
				<!--  Added from 3ed for docx4j 2.7.2, so native Word 2010 docx can be produced -->
				<xsd:annotation>
					<xsd:documentation>Custom Compatibility Setting</xsd:documentation>
				</xsd:annotation>			
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_CompatSetting">
		<xsd:attribute name="name" type="ST_String">
			<xsd:annotation>
				<xsd:documentation>Name of Setting</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="uri" type="ST_String">
			<xsd:annotation>
				<xsd:documentation>Namespace of Setting</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="val" type="ST_String">
			<xsd:annotation>
				<xsd:documentation>Value of Setting</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>	
	<xsd:complexType name="CT_DocVar">
		<xsd:attribute name="name" type="ST_String" use="required">
			<xsd:annotation>
				<xsd:documentation>Document Variable Name</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="val" type="ST_String" use="required">
			<xsd:annotation>
				<xsd:documentation>Document Variable Value</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="CT_DocVars">
		<xsd:sequence>
			<xsd:element name="docVar" type="CT_DocVar" minOccurs="0"
				maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Single Document
						Variable</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_DocRsids">
		<xsd:sequence>
			<xsd:element name="rsidRoot" type="CT_LongHexNumber" minOccurs="0"
				maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Original Document Revision Save
						ID</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="rsid" type="CT_LongHexNumber" minOccurs="0"
				maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Single Session Revision Save
						ID</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="ST_CharacterSpacing">
		<xsd:annotation>
			<xsd:documentation>Character-Level Whitespace Compression
				Settings</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="doNotCompress">
				<xsd:annotation>
					<xsd:documentation>Do Not Compress
						Whitespace</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="compressPunctuation">
				<xsd:annotation>
					<xsd:documentation>Compress Whitespace From Punctuation
						Characters</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="compressPunctuationAndJapaneseKana">
				<xsd:annotation>
					<xsd:documentation>Compress Whitespace From Both Japanese Kana And
						Punctuation Characters</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="CT_CharacterSpacing">
		<xsd:attribute name="val" type="ST_CharacterSpacing" use="required">
			<xsd:annotation>
				<xsd:documentation>Value</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="CT_SaveThroughXslt">
		<xsd:attribute ref="r:id" use="optional">
			<xsd:annotation>
				<xsd:documentation>XSL Transformation Location</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="solutionID" type="ST_String" use="optional">
			<xsd:annotation>
				<xsd:documentation>Local Identifier for XSL
					Transform</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="CT_RPrDefault">
		<xsd:sequence>
			<xsd:element name="rPr" type="CT_RPr" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Run Properties</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_PPrDefault">
		<xsd:sequence>
			<xsd:element name="pPr" type="CT_PPr" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Paragraph Properties</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
