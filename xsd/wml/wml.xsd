	<!--  avoid xjc 2.2.11 collision with w14:shadow  
	
	can do it this way, but its neater to do it in the w14 xsd.
	
	<xsd:element name="shadow" type="BooleanDefaultTrue" >
		<xsd:annotation>
			<xsd:appinfo>
				<jaxb:class name="shadow2006"/>
			</xsd:appinfo>
			<xsd:documentation>Shadow</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	-->
	
	<xsd:group name="EG_RPrBase">
		
		<!-- 
		
		If XJC generates an @XmlElementRef in the class, that item will 
		also get a scoped method in ObjectFactory.
		
		But if XJC generates a field instead, then there won't be
		a corresponding scoped method in ObjectFactory.
		
		How does XJC choose which to do?  It is not entirely clear.
		
		If may have something to do with whether an element is defined 
		inline in this group, ie with name= but not type=
		
		But it looks like its related to whether you have a list or not.
		In a list (eg CTParaRPrOriginal), it is annotated with @XmlElementRefs
		
		If its just a bunch of fields (eg our RPrAbstract), it isn't. 
		
		Not so obvious what the difference here in the xsd is.
		
		-->
		
		<xsd:sequence>
			<xsd:element ref="rStyle" minOccurs="0"/>
			<xsd:element ref="rFonts" minOccurs="0"/>
			<xsd:element name="b" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Bold</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="bCs" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Complex Script Bold</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="i" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Italics</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="iCs" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Complex Script Italics</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="caps" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Display All Characters As Capital
						Letters</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="smallCaps" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Small Caps</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="strike" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Single Strikethrough</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="dstrike" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Double Strikethrough</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="outline" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Display Character
						Outline</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="shadow" type="BooleanDefaultTrue"  minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Shadow</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="emboss" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Embossing</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="imprint" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Imprinting</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="noProof" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Check Spelling or
						Grammar</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="snapToGrid" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Use Document Grid Settings For Inter-Character
						Spacing</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="vanish" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Hidden Text</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="webHidden" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Web Hidden Text</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="color" minOccurs="0"/>
			<xsd:element name="spacing" type="CT_SignedTwipsMeasure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Character Spacing
						Adjustment</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="w" type="CT_TextScale" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Expanded/Compressed
						Text</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="kern" type="CT_HpsMeasure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Font Kerning</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="position" type="CT_SignedHpsMeasure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Vertically Raised or Lowered
						Text</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="sz" type="CT_HpsMeasure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Font Size</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="szCs" type="CT_HpsMeasure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Complex Script Font Size</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="highlight" minOccurs="0"/>
			<xsd:element ref="u" minOccurs="0"/>
			<xsd:element name="effect" type="CT_TextEffect" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Animated Text Effect</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="bdr" type="CT_Border" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Text Border</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="shd" type="CT_Shd" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Run Shading</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="fitText" type="CT_FitText" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Manual Run Width</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="vertAlign" type="CT_VerticalAlignRun" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Subscript/Superscript
						Text</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="rtl" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Right To Left Text</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="cs" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Use Complex Script Formatting on
						Run</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="em" type="CT_Em" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Emphasis Mark</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="lang" type="CT_Language" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Languages for Run
						Content</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="eastAsianLayout" type="CT_EastAsianLayout"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>East Asian Typography
						Settings</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="specVanish" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Paragraph Mark Is Always
						Hidden</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="oMath" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Office Open XML Math</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
<!-- extended by the addition of one or more of the following elements: 

		glow, shadow, reflection, textOutline, textFill, scene3d, props3d, 
		
		ligatures, numForm, numSpacing, stylisticSets, cntxtAlts. 
          
 -->			
			<xsd:group ref="w14:EG_RPrTextEffects" minOccurs="0"></xsd:group>
			<xsd:group ref="w14:EG_RPrOpenType" minOccurs="0"></xsd:group>

		</xsd:sequence>
	</xsd:group>
	
	<xsd:group name="EG_RPrContent">
		<xsd:sequence>
			<xsd:group ref="EG_RPrBase" minOccurs="0"></xsd:group>
			<xsd:element name="rPrChange" type="CT_RPrChange" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Revision Information for Run
						Properties</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	
	<xsd:complexType name="CT_RPr">
		<xsd:annotation>
			<xsd:appinfo>
				<jaxb:class name="RPr"/>
