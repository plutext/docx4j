						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CT_TblPrChange">
		<xsd:complexContent>
			<xsd:extension base="CT_TrackChange">
				<xsd:sequence>
					<xsd:element name="tblPr" type="CT_TblPrBase">
						<xsd:annotation>
							<xsd:documentation>Previous Table
								Properties</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CT_SectPrChange">
		<xsd:complexContent>
			<xsd:extension base="CT_TrackChange">
				<xsd:sequence>
					<xsd:element name="sectPr" type="CT_SectPrBase" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Previous Section
								Properties</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CT_PPrChange">
		<xsd:complexContent>
			<xsd:extension base="CT_TrackChange">
				<xsd:sequence>
					<xsd:element name="pPr" type="CT_PPrBase" minOccurs="1">
						<xsd:annotation>
							<xsd:documentation>Previous Paragraph
								Properties</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CT_RPrChange">
		<xsd:complexContent>
			<xsd:extension base="CT_TrackChange">
				<xsd:sequence>
					<xsd:element name="rPr" minOccurs="1">
						<xsd:annotation>
							<xsd:documentation>Previous Run
								Properties</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:group ref="EG_RPrBase" minOccurs="0"
									maxOccurs="unbounded">
								</xsd:group>
							</xsd:sequence>
						</xsd:complexType>
						
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CT_ParaRPrChange">
		<xsd:annotation>
			<xsd:appinfo>
				<jaxb:class name="ParaRPrChange"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="CT_TrackChange">
				<xsd:sequence>
					<xsd:element name="rPr" type="CT_ParaRPrOriginal"
						minOccurs="1">
						<xsd:annotation>
							<xsd:documentation>Previous Run Properties for the
								Paragraph Mark</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CT_RunTrackChange">
		<xsd:annotation>
			<xsd:appinfo>
				<jaxb:class name="RunTrackChange"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="CT_TrackChange">
				<xsd:choice minOccurs="0" maxOccurs="unbounded">
					<xsd:group ref="m:EG_OMathMathElements" />
					<xsd:group ref="EG_ContentRunContent"></xsd:group>
				</xsd:choice>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="EG_CellMarkupElements">
		<xsd:choice>
			<xsd:element name="cellIns" type="CT_TrackChange" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Table Cell Insertion</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="cellDel" type="CT_TrackChange" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Table Cell Deletion</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="cellMerge" type="CT_CellMergeTrackChange"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Vertically Merged/Split Table
						Cells</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:group>
	
			<xsd:element name="commentRangeStart">
				<xsd:annotation>
					<xsd:documentation>
						Comment Anchor Range Start
					</xsd:documentation>
					<!-- 
					<xsd:appinfo>
						<jaxb:class name="CommentRangeStart" />
					</xsd:appinfo>  -->
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="id" type="ST_DecimalNumber"
						use="required">
						<xsd:annotation>
							<xsd:documentation>
								Annotation Identifier
							</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="displacedByCustomXml"
						type="ST_DisplacedByCustomXml" use="optional">
						<xsd:annotation>
							<xsd:documentation>
								Annotation Marker Relocated For Custom
								XML Markup
							</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="commentRangeEnd">
				<xsd:annotation>
					<xsd:documentation>
								Comment Anchor Range End
					</xsd:documentation>
					<!-- 
					<xsd:appinfo>
						<jaxb:class name="CommentRangeEnd" />
					</xsd:appinfo>  -->
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="id" type="ST_DecimalNumber"
						use="required">
						<xsd:annotation>
							<xsd:documentation>
								Annotation Identifier
							</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="displacedByCustomXml"
						type="ST_DisplacedByCustomXml" use="optional">
						<xsd:annotation>
							<xsd:documentation>
								Annotation Marker Relocated For Custom
								XML Markup
							</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:complexType>
			</xsd:element>
	
	
	<xsd:group name="EG_RangeMarkupElements">
		<xsd:choice>
			<xsd:element name="bookmarkStart" type="CT_Bookmark">
				<xsd:annotation>
					<xsd:documentation>Bookmark Start</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="bookmarkEnd" type="CT_MarkupRange">
				<xsd:annotation>
					<xsd:documentation>Bookmark End</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="moveFromRangeStart" type="CT_MoveBookmark">
				<xsd:annotation>
					<xsd:documentation>Move Source Location Container -
						Start</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="moveFromRangeEnd" type="CT_MoveFromRangeEnd">
				<xsd:annotation>
					<xsd:documentation>Move Source Location Container -
						End</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="moveToRangeEnd" type="CT_MoveToRangeEnd">
				<xsd:annotation>
					<xsd:documentation>Move Destination Location Container -
						End</xsd:documentation>
				</xsd:annotation>
			</xsd:element>			
			<xsd:element name="moveToRangeStart" type="CT_MoveBookmark">
				<xsd:annotation>
					<xsd:documentation>Move Destination Location Container -
						Start</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="commentRangeStart"/>
			<xsd:element ref="commentRangeEnd" />
			<xsd:element name="customXmlInsRangeStart" type="CT_TrackChange">
				<xsd:annotation>
					<xsd:documentation>Custom XML Markup Insertion
						Start</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="customXmlInsRangeEnd" type="CT_Markup">
				<xsd:annotation>
					<xsd:documentation>Custom XML Markup Insertion
						End</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="customXmlDelRangeStart" type="CT_TrackChange">
				<xsd:annotation>
					<xsd:documentation>Custom XML Markup Deletion
						Start</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="customXmlDelRangeEnd" type="CT_Markup">
				<xsd:annotation>
					<xsd:documentation>Custom XML Markup Deletion
						End</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="customXmlMoveFromRangeStart" type="CT_TrackChange">
				<xsd:annotation>
					<xsd:documentation>Custom XML Markup Move Source
						Start</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="customXmlMoveFromRangeEnd" type="CT_Markup">
				<xsd:annotation>
					<xsd:documentation>Custom XML Markup Move Source
						End</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="customXmlMoveToRangeStart" type="CT_TrackChange">
				<xsd:annotation>
					<xsd:documentation>Custom XML Markup Move Destination Location
						Start</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="customXmlMoveToRangeEnd" type="CT_Markup">
				<xsd:annotation>
					<xsd:documentation>Custom XML Markup Move Destination Location
						End</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:group>
	<xsd:complexType name="CT_Tabs">
		<xsd:annotation>
			<xsd:appinfo>
				<jaxb:class name="Tabs"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="tab" type="CT_TabStop" minOccurs="1"
				maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Custom Tab Stop</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="ST_TextboxTightWrap">
		<xsd:annotation>
			<xsd:documentation>Lines To Tight Wrap Within Text Box</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="none">
				<xsd:annotation>
					<xsd:documentation>Do Not Tight Wrap</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="allLines">
				<xsd:annotation>
					<xsd:documentation>Tight Wrap All Lines</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="firstAndLastLine">
				<xsd:annotation>
					<xsd:documentation>Tight Wrap First and Last
						Lines</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="firstLineOnly">
				<xsd:annotation>
					<xsd:documentation>Tight Wrap First Line</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="lastLineOnly">
				<xsd:annotation>
					<xsd:documentation>Tight Wrap Last Line</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="CT_TextboxTightWrap">
		<xsd:attribute name="val" type="ST_TextboxTightWrap" use="required">
			<xsd:annotation>
				<xsd:documentation>Lines to Tight Wrap to Paragraph
					Extents</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	
	<xsd:complexType name="CT_PPrBase">
		<xsd:annotation>
			<xsd:appinfo>
				<jaxb:class name="PPrBase"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="pStyle" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Referenced Paragraph
						Style</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="val" type="xsd:string"/>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="keepNext" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Keep Paragraph With Next
						Paragraph</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="keepLines" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Keep All Lines On One Page</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="pageBreakBefore" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Start Paragraph on Next
						Page</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="framePr" type="CT_FramePr" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Text Frame Properties</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="widowControl" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Allow First/Last Line to Display on a Separate
						Page</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="numPr" minOccurs="0"> <!-- type="CT_NumPr" -->
				<xsd:annotation>
					<xsd:documentation>Numbering Definition Instance
						Reference</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="ilvl" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Numbering Level
									Reference</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:attribute name="val" use="required">
									<xsd:simpleType>
										
										<xsd:restriction base="xsd:integer"/>
										
									</xsd:simpleType>
								</xsd:attribute>
							</xsd:complexType>
							
						</xsd:element>
						<xsd:element name="numId" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Numbering Definition Instance
									Reference</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:attribute name="val" use="required">
									<xsd:simpleType>
										<xsd:restriction base="xsd:integer"/>
									</xsd:simpleType>
								</xsd:attribute>
							</xsd:complexType>
							
						</xsd:element>
						<xsd:element name="numberingChange"
							type="CT_TrackChangeNumbering" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Previous Paragraph Numbering
									Properties</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ins" type="CT_TrackChange"
							minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Inserted Numbering
									Properties</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="suppressLineNumbers" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Suppress Line Numbers for
						Paragraph</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="pBdr" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Paragraph Borders</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="top" type="CT_Border" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Paragraph Border Above Identical
									Paragraphs</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="left" type="CT_Border" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Left Paragraph
									Border</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="bottom" type="CT_Border" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Paragraph Border Between
									Identical Paragraphs</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="right" type="CT_Border" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Right Paragraph
									Border</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="between" type="CT_Border" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Paragraph Border Between
									Identical Paragraphs</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="bar" type="CT_Border" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Paragraph Border Between Facing
									Pages</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="shd" type="CT_Shd" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Paragraph Shading</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="tabs" type="CT_Tabs" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Set of Custom Tab Stops</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="suppressAutoHyphens" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Suppress Hyphenation for
						Paragraph</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="kinsoku" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Use East Asian Typography Rules for First and
						Last Character per Line</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="wordWrap" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Allow Line Breaking At Character
						Level</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="overflowPunct" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Allow Punctuation to Extent Past Text
						Extents</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="topLinePunct" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Compress Punctuation at Start of a
						Line</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="autoSpaceDE" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Automatically Adjust Spacing of Latin and East
						Asian Text</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="autoSpaceDN" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Automatically Adjust Spacing of East Asian Text
						and Numbers</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="bidi" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Right to Left Paragraph
						Layout</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="adjustRightInd" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Automatically Adjust Right Indent When Using
						Document Grid</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="snapToGrid" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Use Document Grid Settings for Inter-Line
						Paragraph Spacing</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="spacing" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Spacing Between Lines and Above/Below
						Paragraph</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="before" type="ST_TwipsMeasure"
						use="optional">
						<xsd:annotation>
							<xsd:documentation>Spacing Above
								Paragraph</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="beforeLines" type="ST_DecimalNumber"
						use="optional">
						<xsd:annotation>
							<xsd:documentation>Spacing Above Paragraph IN Line
								Units</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="beforeAutospacing" type="xsd:boolean"
						default="true" use="optional">
						<xsd:annotation>
							<xsd:documentation>Automatically Determine Spacing
								Above Paragraph</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="after" type="ST_TwipsMeasure"
						use="optional">
						<xsd:annotation>
							<xsd:documentation>Spacing Below
								Paragraph</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="afterLines" type="ST_DecimalNumber"
						use="optional">
						<xsd:annotation>
							<xsd:documentation>Spacing Below Paragraph in Line
								Units</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="afterAutospacing" type="xsd:boolean"
						default="true" use="optional">
						<xsd:annotation>
							<xsd:documentation>Automatically Determine Spacing
								Below Paragraph</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="line" type="ST_SignedTwipsMeasure"
						use="optional">
						<xsd:annotation>
							<xsd:documentation>Spacing Between Lines in
								Paragraph</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="lineRule" type="ST_LineSpacingRule"
						use="optional">
						<xsd:annotation>
							<xsd:documentation>Type of Spacing Between
								Lines</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="ind" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Paragraph Indentation</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="left" type="ST_SignedTwipsMeasure"
						use="optional">
						<xsd:annotation>
							<xsd:documentation>Left
								Indentation</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="leftChars" type="ST_DecimalNumber"
						use="optional">
						<xsd:annotation>
							<xsd:documentation>Left Indentation in Character
								Units</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="right" type="ST_SignedTwipsMeasure"
						use="optional">
						<xsd:annotation>
							<xsd:documentation>Right
								Indentation</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="rightChars" type="ST_DecimalNumber"
						use="optional">
						<xsd:annotation>
							<xsd:documentation>Right Indentation in Character
								Units</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="hanging" type="ST_TwipsMeasure"
						use="optional">
						<xsd:annotation>
							<xsd:documentation>Indentation Removed from First
								Line</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="hangingChars" type="ST_DecimalNumber"
						use="optional">
						<xsd:annotation>
							<xsd:documentation>Indentation Removed From First Line in
								Character Units</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="firstLine" type="ST_TwipsMeasure"
						use="optional">
						<xsd:annotation>
							<xsd:documentation>Additional First Line
								Indentation</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="firstLineChars" type="ST_DecimalNumber"
						use="optional">
						<xsd:annotation>
							<xsd:documentation>Additional First Line Indentation in
								Character Units</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="contextualSpacing" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Ignore Spacing Above and Below When Using
						Identical Styles</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="mirrorIndents" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Use Left/Right Indents as Inside/Outside
						Indents</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="suppressOverlap" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Prevent Text Frames From
						Overlapping</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="jc" type="CT_Jc" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Paragraph Alignment</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="textDirection" type="CT_TextDirection"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Paragraph Text Flow
						Direction</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="textAlignment" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Vertical Character Alignment on
						Line</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="val" use="required">
						<xsd:annotation>
							<xsd:documentation>Vertical Character Alignment
								Position</xsd:documentation>
						</xsd:annotation>
						<xsd:simpleType>
							<xsd:annotation>
								<xsd:documentation>Vertical Text Alignment
									Types</xsd:documentation>
							</xsd:annotation>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value="top">
									<xsd:annotation>
										<xsd:documentation>Align Text at
											Top</xsd:documentation>
									</xsd:annotation>
								</xsd:enumeration>
								<xsd:enumeration value="center">
									<xsd:annotation>
										<xsd:documentation>Align Text at
											Center</xsd:documentation>
									</xsd:annotation>
								</xsd:enumeration>
								<xsd:enumeration value="baseline">
									<xsd:annotation>
										<xsd:documentation>Align Text at
											Baseline</xsd:documentation>
									</xsd:annotation>
								</xsd:enumeration>
								<xsd:enumeration value="bottom">
									<xsd:annotation>
										<xsd:documentation>Align Text at
											Bottom</xsd:documentation>
									</xsd:annotation>
								</xsd:enumeration>
								<xsd:enumeration value="auto">
									<xsd:annotation>
										<xsd:documentation>Automatically
											Determine
											Alignment</xsd:documentation>
									</xsd:annotation>
								</xsd:enumeration>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="textboxTightWrap" type="CT_TextboxTightWrap"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Allow Surrounding Paragraphs to Tight Wrap to
						Text Box Contents</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="outlineLvl" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Associated Outline
						Level</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="val" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:integer"/>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="divId" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Associated HTML div ID</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="val" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:integer"/>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="cnfStyle" type="CT_Cnf" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Paragraph Conditional
						Formatting</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<xsd:element ref="w15:collapsed" minOccurs="0"  /> <!--  experimentation shows that Word places this before w:pPr/w:rPr -->
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="CT_PPr">
		<xsd:annotation>
			<xsd:appinfo>
				<jaxb:class name="PPr"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="CT_PPrBase">
				<xsd:sequence>
					<xsd:element name="rPr" type="CT_ParaRPr" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Run Properties for the Paragraph
								Mark</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="sectPr" type="CT_SectPr" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Section
								Properties</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="pPrChange" type="CT_PPrChange"
						minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Revision Information for Paragraph
								Properties</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CT_Control">
		<xsd:attribute name="name" type="ST_String" use="optional">
			<xsd:annotation>
				<xsd:documentation>Unique Name for Embedded
					Control</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="shapeid" type="ST_String" use="optional">
			<xsd:annotation>
				<xsd:documentation>Associated VML Data
					Reference</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute ref="r:id" use="optional">
			<xsd:annotation>
				<xsd:documentation>Embedded Control Properties Relationship
					Reference</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="CT_Background">
		<xsd:complexContent>
			<xsd:extension base="CT_PictureBase">
				<xsd:attribute name="color" type="ST_HexColor" use="optional">
					<xsd:annotation>
						<xsd:documentation>Background Color</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="themeColor" type="ST_ThemeColor"
					use="optional">
					<xsd:annotation>
						<xsd:documentation>Background Theme
							Color</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="themeTint" type="ST_UcharHexNumber"
					use="optional">
					<xsd:annotation>
						<xsd:documentation>Border Theme Color
							Tint</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="themeShade" type="ST_UcharHexNumber"
					use="optional">
					<xsd:annotation>
						<xsd:documentation>Border Theme Color
							Shade</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CT_Rel">
		<xsd:attribute ref="r:id" use="required">
			<xsd:annotation>
				<xsd:documentation>Relationship to Part</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="CT_PictureBase">
		<xsd:sequence maxOccurs="unbounded">
			<xsd:any processContents="lax"
				namespace="urn:schemas-microsoft-com:vml" minOccurs="0"/>
			<xsd:any processContents="lax"
				namespace="urn:schemas-microsoft-com:office:office"
				minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_Object">
		<xsd:complexContent>
			<xsd:extension base="CT_PictureBase">
				<xsd:sequence>
					<xsd:element name="control" type="CT_Control" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Inline Embedded
								Control</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute name="dxaOrig" type="ST_TwipsMeasure"
					use="optional">
					<xsd:annotation>
						<xsd:documentation>Original Image
							Width</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="dyaOrig" type="ST_TwipsMeasure"
					use="optional">
					<xsd:annotation>
						<xsd:documentation>Original Image
							Height</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute ref="w14:anchorId" use="optional"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CT_Picture">
		<xsd:annotation>
			<xsd:appinfo>
				<jaxb:class name="pict"/>
			</xsd:appinfo>
		</xsd:annotation>	
		<xsd:complexContent>
			<xsd:extension base="CT_PictureBase">
				<xsd:sequence maxOccurs="1">
					<xsd:element name="movie" type="CT_Rel" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Embedded Video</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="control" type="CT_Control" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Floating Embedded
								Control</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute ref="w14:anchorId" use="optional"/>
			</xsd:extension>			
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:complexType name="CT_Drawing">
		<xsd:annotation>
			<xsd:appinfo>
				<jaxb:class name="drawing"/>
			</xsd:appinfo>
		</xsd:annotation>	
		<xsd:choice minOccurs="1" maxOccurs="unbounded">
			<xsd:element ref="wp:anchor" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						Drawing Element Anchor
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="wp:inline" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						Inline Drawing Object
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:complexType>
	
	<xsd:complexType name="CT_SimpleField">
		<xsd:sequence>
			<xsd:element name="fldData" type="CT_Text" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Custom Field Data</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:group ref="EG_PContent" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="instr" type="ST_String" use="required">
			<xsd:annotation>
				<xsd:documentation>Field Codes</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="fldLock" type="xsd:boolean" default="true">
			<xsd:annotation>
				<xsd:documentation>Field Should Not Be
					Recalculated</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="dirty" type="xsd:boolean" default="true">
			<xsd:annotation>
				<xsd:documentation>Field Result Invalidated</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:simpleType name="ST_FldCharType">
		<xsd:annotation>
			<xsd:documentation>Complex Field Character Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="begin">
				<xsd:annotation>
					<xsd:documentation>Start Character</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="separate">
				<xsd:annotation>
					<xsd:documentation>Separator Character</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="end">
				<xsd:annotation>
					<xsd:documentation>End Character</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ST_InfoTextType">
		<xsd:annotation>
			<xsd:documentation>Help or Status Text Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="text">
				<xsd:annotation>
					<xsd:documentation>Literal Text</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="autoText">
				<xsd:annotation>
					<xsd:documentation>Glossary Document Entry</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ST_FFHelpTextVal">
		<xsd:annotation>
			<xsd:documentation>Help Text Value</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="256"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ST_FFStatusTextVal">
		<xsd:annotation>
			<xsd:documentation>Status Text Value</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="140"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ST_FFName">
		<xsd:annotation>
			<xsd:documentation>Form Field Name Value</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="65"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ST_FFTextType">
		<xsd:annotation>
			<xsd:documentation>Text Box Form Field Type Values</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="regular">
				<xsd:annotation>
					<xsd:documentation>Text Box</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="number">
				<xsd:annotation>
					<xsd:documentation>Number</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="date">
				<xsd:annotation>
					<xsd:documentation>Date</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="currentTime">
				<xsd:annotation>
					<xsd:documentation>Current Time Display</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="currentDate">
				<xsd:annotation>
					<xsd:documentation>Current Date Display</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="calculated">
				<xsd:annotation>
					<xsd:documentation>Field Calculation</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="CT_FFTextType">
		<xsd:attribute name="val" type="ST_FFTextType" use="required">
			<xsd:annotation>
				<xsd:documentation>Text Box Form Field Type
					Values</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="CT_FFName">
		<xsd:attribute name="val" type="ST_FFName">
			<xsd:annotation>
				<xsd:documentation>Form Field Name Value</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="CT_FldChar">
  		<xsd:annotation>
			<xsd:appinfo>
				<jaxb:class name="FldChar"/>
			</xsd:appinfo>
		</xsd:annotation>	
		<xsd:choice>
			<xsd:element name="fldData" type="CT_Text" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Custom Field Data</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ffData" type="CT_FFData" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Form Field Properties</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="numberingChange" type="CT_TrackChangeNumbering"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Previous Numbering Field
						Properties</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:choice>
		<xsd:attribute name="fldCharType" type="ST_FldCharType" use="required">
			<xsd:annotation>
				<xsd:documentation>Field Character Type</xsd:documentation>
			</xsd:annotation>
