					<xsd:documentation>Number of Pages Per
						Booklet</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="val" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:integer"/>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="drawingGridHorizontalSpacing"
				type="CT_TwipsMeasure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Drawing Grid Horizontal Grid Unit
						Size</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="drawingGridVerticalSpacing" type="CT_TwipsMeasure"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Drawing Grid Vertical Grid Unit
						Size</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="displayHorizontalDrawingGridEvery" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Distance between Horizontal
						Gridlines</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="val" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:integer"/>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="displayVerticalDrawingGridEvery" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Distance between Vertical
						Gridlines</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="val" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:integer"/>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="doNotUseMarginsForDrawingGridOrigin"
				type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Use Margins for Drawing Grid
						Origin</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="drawingGridHorizontalOrigin" type="CT_TwipsMeasure"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Drawing Grid Horizontal Origin
						Point</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="drawingGridVerticalOrigin" type="CT_TwipsMeasure"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Drawing Grid Vertical Origin
						Point</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doNotShadeFormData" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Show Visual Indicator For Form
						Fields</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="noPunctuationKerning" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Never Kern Punctuation
						Characters</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="characterSpacingControl" type="CT_CharacterSpacing"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Character-Level Whitespace
						Compression</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="printTwoOnOne" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Print Two Pages Per Sheet</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="strictFirstAndLastChars" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Use Strict Kinsoku Rules for Japanese
						Text</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="noLineBreaksAfter" type="CT_Kinsoku" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Custom Set of Characters Which Cannot End a
						Line</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="noLineBreaksBefore" type="CT_Kinsoku" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Custom Set Of Characters Which Cannot Begin A
						Line</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="savePreviewPicture" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Generate Thumbnail For Document On
						Save</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doNotValidateAgainstSchema"
				type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Validate Custom XML Markup Against
						Schemas</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="saveInvalidXml" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Allow Saving Document As XML File When Custom XML
						Markup Is Invalid</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ignoreMixedContent" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Ignore Mixed Content When Validating Custom XML
						Markup</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="alwaysShowPlaceholderText"
				type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Use Custom XML Element Names as Default
						Placeholder Text</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doNotDemarcateInvalidXml" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Show Visual Indicator For Invalid Custom
						XML Markup</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="saveXmlDataOnly" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Only Save Custom XML
						Markup</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="useXSLTWhenSaving" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Save Document as XML File through Custom XSL
						Transform</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="saveThroughXslt" type="CT_SaveThroughXslt"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Custom XSL Transform To Use When Saving As XML
						File</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="showXMLTags" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Show Visual Indicators for Custom XML Markup
						Start/End Locations</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="alwaysMergeEmptyNamespace"
				type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Mark Custom XML Elements With No Namespace
						As Invalid</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="updateFields" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Automatically Recalculate Fields on
						Open</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="hdrShapeDefaults" type="CT_ShapeDefaults"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Default Properties for VML Objects in Header and
						Footer</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="footnotePr" type="CT_FtnDocProps" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Document-Wide Footnote
						Properties</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="endnotePr" type="CT_EdnDocProps" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Document-Wide Endnote
						Properties</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="compat" type="CT_Compat" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Compatibility Settings</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="docVars" type="CT_DocVars" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Document Variables</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="rsids" type="CT_DocRsids" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Listing of All Revision Save ID
						Values</xsd:documentation>
				</xsd:annotation>
			</xsd:element> 
			<xsd:element ref="m:mathPr" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>properties of math in the document</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="uiCompat97To2003" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Disable Features Incompatible With Earlier
						Word Processing Formats</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="attachedSchema" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Attached Custom XML
						Schema</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="val" type="xsd:string"/>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="themeFontLang" type="CT_Language" minOccurs="0"
				maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Theme Font Languages</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="clrSchemeMapping" type="CT_ColorSchemeMapping"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Theme Color Mappings</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doNotIncludeSubdocsInStats"
				type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Include Content in Text Boxes, Footnotes,
						and Endnotes in Document Statistics</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doNotAutoCompressPictures"
				type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Automatically Compress
						Images</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="forceUpgrade" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Upgrade Document on Open</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType/>
			</xsd:element>
			<xsd:element name="captions" type="CT_Captions" minOccurs="0"
				maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Caption Settings</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="readModeInkLockDown"
				type="CT_ReadingModeInkLockDown" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Freeze Document Layout</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="smartTagType" type="CT_SmartTagType" minOccurs="0"
				maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Supplementary Smart Tag
						Information</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="sl:schemaLibrary" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Custom XML Schema List</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="shapeDefaults" type="CT_ShapeDefaults"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Default Properties for VML Objects in Main
						Document</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doNotEmbedSmartTags" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Remove Smart Tags When
						Saving</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="decimalSymbol" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Radix Point for Field Code
						Evaluation</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="val" type="xsd:string"/>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="listSeparator" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>List Separator for Field Code
						Evaluation</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="val" type="xsd:string"/>
				</xsd:complexType>
				
			</xsd:element>
			
	        <xsd:element ref="w15:chartTrackingRefBased" minOccurs="0" maxOccurs="1"/>
	        <xsd:element ref="w14:docId" minOccurs="0" maxOccurs="1">
		       <xsd:annotation>
		          <xsd:appinfo>
		             <jaxb:property name="docId14"/>
		          </xsd:appinfo>
		       </xsd:annotation>
		   </xsd:element>
   	        
	        <xsd:element ref="w15:docId" minOccurs="0" maxOccurs="1">
		       <xsd:annotation>
		          <xsd:appinfo>
		             <jaxb:property name="docId15" />
		          </xsd:appinfo>
		       </xsd:annotation>
		   </xsd:element>
	        
	        <xsd:element ref="w14:conflictMode" minOccurs="0" maxOccurs="1"/>
	        <xsd:element ref="w14:discardImageEditingData" minOccurs="0" maxOccurs="1"/>
	        <xsd:element ref="w14:defaultImageDpi" minOccurs="0" maxOccurs="1"/>
        			
		</xsd:sequence>
		<xsd:attribute ref="mc:Ignorable" use="optional" />
		
	</xsd:complexType>
	<xsd:complexType name="CT_WebSettings">
		<xsd:sequence>
			<xsd:element name="frameset" type="CT_Frameset" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Root Frameset
						Definition</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="divs" type="CT_Divs" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Information about HTML div
						Elements</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="encoding" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Output Encoding When Saving as Web
						Page</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="val" type="xsd:string"/>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="optimizeForBrowser" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Disable Features Not Supported by Target Web
						Browser</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="relyOnVML" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Utilize VML When Saving as Web
						Page</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="allowPNG" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Allow PNG as Graphic
						Format</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doNotRelyOnCSS" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Rely on CSS for Font Face
						Formatting</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doNotSaveAsSingleFile" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Recommend Web Page Format over Single File Web
						Page Format</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doNotOrganizeInFolder" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Place Supporting Files in
						Subdirectory</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doNotUseLongFileNames" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Use File Names Longer than 8.3
						Characters</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="pixelsPerInch" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Pixels per Inch for
						Graphics/Images</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="val" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:integer"/>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="targetScreenSz" type="CT_TargetScreenSz"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Target Screen Size for Web
						Page</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="saveSmartTagsAsXml" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Save Smart Tag Data in XML Property
						Bag</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="ST_FrameScrollbar">
		<xsd:annotation>
			<xsd:documentation>Frame Scrollbar Visibility</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="on">
				<xsd:annotation>
					<xsd:documentation>Always Show Scrollbar</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="off">
				<xsd:annotation>
					<xsd:documentation>Never Show Scrollbar</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="auto">
				<xsd:annotation>
					<xsd:documentation>Automatically Show Scrollbar As
						Needed</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="CT_FrameScrollbar">
		<xsd:attribute name="val" type="ST_FrameScrollbar" use="required">
			<xsd:annotation>
				<xsd:documentation>Scrollbar Display Option
					Value</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="CT_Frame">
		<xsd:sequence>
			<xsd:element name="sz" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Frame Size</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="val" type="xsd:string"/>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="name" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Frame Name</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="val" type="xsd:string"/>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="sourceFileName" type="CT_Rel" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Source File for Frame</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="marW" type="CT_PixelsMeasure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Left and Right Margin for
						Frame</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="marH" type="CT_PixelsMeasure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Top and Bottom Margin for
						Frame</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="scrollbar" type="CT_FrameScrollbar" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Scrollbar Display
						Option</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="noResizeAllowed" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Frame Cannot Be Resized</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="linkedToFile" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Maintain Link to Existing
						File</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="ST_FrameLayout">
		<xsd:annotation>
			<xsd:documentation>Frameset Layout Order</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="rows">
				<xsd:annotation>
					<xsd:documentation>Stack Frames Vertically</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="cols">
				<xsd:annotation>
					<xsd:documentation>Stack Frames
						Horizontally</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="none">
				<xsd:annotation>
					<xsd:documentation>Do Not Stack Frames</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="CT_FrameLayout">
		<xsd:attribute name="val" type="ST_FrameLayout" use="required">
			<xsd:annotation>
				<xsd:documentation>Frameset Layout Value</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="CT_FramesetSplitbar">
		<xsd:sequence>
			<xsd:element name="w" type="CT_TwipsMeasure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Frameset Splitter Width</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="color" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Frameset Splitter Color</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="val" type="ST_HexColor" use="required">
						<xsd:annotation>
							<xsd:documentation>Run Content
								Color</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="themeColor" type="ST_ThemeColor"
						use="optional">
						<xsd:annotation>
							<xsd:documentation>Run Content Theme
								Color</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="themeTint" type="ST_UcharHexNumber"
						use="optional">
						<xsd:annotation>
							<xsd:documentation>Run Content Theme Color
								Tint</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="themeShade" type="ST_UcharHexNumber"
						use="optional">
						<xsd:annotation>
							<xsd:documentation>Run Content Theme Color
								Shade</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="noBorder" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do Not Display Frameset
						Splitters</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="flatBorders" type="BooleanDefaultTrue"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Frameset Splitter Border
						Style</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_Frameset">
		<xsd:sequence>
			<xsd:element name="sz" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Nested Frameset Size</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="val" type="xsd:string"/>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="framesetSplitbar" type="CT_FramesetSplitbar"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Frameset Splitter
						Properties</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="frameLayout" type="CT_FrameLayout" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Frameset Layout</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element name="frameset" type="CT_Frameset" minOccurs="0"
					maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>Nested Frameset
							Definition</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="frame" type="CT_Frame" minOccurs="0"
					maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>Single Frame
							Properties</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_Lvl">
		<!-- Numbering Level Definition -->
		<!-- This is used in two places, so keep it -->
		<xsd:annotation>
			<xsd:appinfo>
				<jaxb:class name="Lvl"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="start" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Starting Value</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="val" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:integer"/>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="numFmt" type="CT_NumFmt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Numbering Format</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="lvlRestart" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Restart Numbering Level
						Symbol</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="val" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:integer"/>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="pStyle" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Paragraph Style's Associated Numbering
						Level</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="val" type="xsd:string"/>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="isLgl" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Display All Levels Using Arabic
						Numerals</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="suff" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Content Between Numbering Symbol and Paragraph
						Text</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="val" use="required">
						<xsd:annotation>
							<xsd:documentation>Character Type Between Numbering and
								Text</xsd:documentation>
						</xsd:annotation>
						<xsd:simpleType>
							<xsd:annotation>
								<xsd:documentation>Content Between Numbering Symbol
									and Paragraph Text</xsd:documentation>
							</xsd:annotation>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value="tab">
									<xsd:annotation>
										<xsd:documentation>Tab Between Numbering
											and Text</xsd:documentation>
									</xsd:annotation>
								</xsd:enumeration>
								<xsd:enumeration value="space">
									<xsd:annotation>
										<xsd:documentation>Space Between Numbering
											and Text</xsd:documentation>
									</xsd:annotation>
								</xsd:enumeration>
								<xsd:enumeration value="nothing">
									<xsd:annotation>
										<xsd:documentation>Nothing Between
											Numbering and Text</xsd:documentation>
									</xsd:annotation>
								</xsd:enumeration>
							</xsd:restriction>
						</xsd:simpleType>
						
					</xsd:attribute>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="lvlText" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Numbering Level Text</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="val" type="ST_String" use="optional">
						<xsd:annotation>
							<xsd:documentation>Level Text</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="null" type="xsd:boolean" default="true"
						use="optional">
						<xsd:annotation>
							<xsd:documentation>Level Text Is Null
								Character</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="lvlPicBulletId" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Picture Numbering Symbol Definition
						Reference</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="val" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:integer"/>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="legacy" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Legacy Numbering Level
						Properties</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="legacy" type="xsd:boolean" default="true"
						use="optional">
						<xsd:annotation>
							<xsd:documentation>Use Legacy Numbering
								Properties</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="legacySpace" type="ST_TwipsMeasure"
						use="optional">
						<xsd:annotation>
							<xsd:documentation>Legacy Spacing</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="legacyIndent"
						type="ST_SignedTwipsMeasure" use="optional">
						<xsd:annotation>
							<xsd:documentation>Legacy Indent</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:complexType>
				
			</xsd:element>
			<xsd:element name="lvlJc" type="CT_Jc" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation> Justification</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="pPr" type="CT_PPr" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Numbering Level Associated Paragraph
						Properties</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="rPr" type="CT_RPr" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Numbering Symbol Run
						Properties</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="ilvl" type="ST_DecimalNumber" use="required">
			<xsd:annotation>
				<xsd:documentation>Numbering Level</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="tplc" type="ST_LongHexNumber" use="optional">
			<xsd:annotation>
				<xsd:documentation>Template Code</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="tentative" type="xsd:boolean" default="true"
			use="optional">
			<xsd:annotation>
				<xsd:documentation>Tentative Numbering</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:simpleType name="ST_TblStyleOverrideType">
		<xsd:annotation>
			<xsd:documentation>Conditional Table Style Formatting
				Types</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="wholeTable">
				<xsd:annotation>
					<xsd:documentation>Whole table formatting</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="firstRow">
				<xsd:annotation>
					<xsd:documentation>First Row Conditional
						Formatting</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="lastRow">
				<xsd:annotation>
					<xsd:documentation>Last table row
						formatting</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="firstCol">
				<xsd:annotation>
					<xsd:documentation>First Column Conditional
						Formatting</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="lastCol">
				<xsd:annotation>
					<xsd:documentation>Last table column
						formatting</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="band1Vert">
				<xsd:annotation>
					<xsd:documentation>Banded Column Conditional
						Formatting</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="band2Vert">
				<xsd:annotation>
					<xsd:documentation>Even Column Stripe Conditional
						Formatting</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="band1Horz">
				<xsd:annotation>
					<xsd:documentation>Banded Row Conditional
						Formatting</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="band2Horz">
				<xsd:annotation>
					<xsd:documentation>Even Row Stripe Conditional
						Formatting</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="neCell">
				<xsd:annotation>
					<xsd:documentation>Top right table cell
						formatting</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="nwCell">
				<xsd:annotation>
					<xsd:documentation>Top left table cell
						formatting</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="seCell">
				<xsd:annotation>
					<xsd:documentation>Bottom right table cell
						formatting</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="swCell">
				<xsd:annotation>
					<xsd:documentation>Bottom left table cell
						formatting</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="CT_TblStylePr">
		<xsd:sequence>
			<xsd:element name="pPr" type="CT_PPr" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Table Style Conditional Formatting Paragraph
						Properties</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="rPr" type="CT_RPr" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Table Style Conditional Formatting Run
						Properties</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="tblPr" type="CT_TblPrBase" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Table Style Conditional Formatting Table
						Properties</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="trPr" type="CT_TrPr" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Table Style Conditional Formatting Table Row
						Properties</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="tcPr" type="CT_TcPr" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Table Style Conditional Formatting Table Cell
						Properties</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="type" type="ST_TblStyleOverrideType" use="required">
			<xsd:annotation>
				<xsd:documentation>Table Style Conditional Formatting
					Type</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:simpleType name="ST_Panose">
		<xsd:annotation>
			<xsd:documentation>Panose-1 Number</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:hexBinary">
			<xsd:length value="10"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="CT_Panose">
		<xsd:annotation>
			<xsd:appinfo>
				<jaxb:class name="FontPanose"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:attribute name="val" type="ST_Panose" use="required">
			<xsd:annotation>
				<xsd:documentation>Value</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="CT_FontFamily">
		<xsd:annotation>
			<xsd:appinfo>
				<jaxb:class name="FontFamily"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:attribute name="val" use="required">
			<xsd:annotation>
				<xsd:documentation>Font Family Value</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:annotation>
					<xsd:documentation>Font Family Value</xsd:documentation>
				</xsd:annotation>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="decorative">
						<xsd:annotation>
							<xsd:documentation>Novelty Font</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
					<xsd:enumeration value="modern">
						<xsd:annotation>
							<xsd:documentation>Monospace Font</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
					<xsd:enumeration value="roman">
						<xsd:annotation>
							<xsd:documentation>Proportional Font With
								Serifs</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
					<xsd:enumeration value="script">
						<xsd:annotation>
							<xsd:documentation>Script Font</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
					<xsd:enumeration value="swiss">
						<xsd:annotation>
							<xsd:documentation>Proportional Font Without
								Serifs</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
					<xsd:enumeration value="auto">
						<xsd:annotation>
							<xsd:documentation>No Font Family</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:simpleType name="ST_Pitch">
		<xsd:annotation>
			<xsd:documentation>Font Pitch Value</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="fixed">
				<xsd:annotation>
					<xsd:documentation>Fixed Width</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="variable">
				<xsd:annotation>
					<xsd:documentation>Proportional Width</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="default">
				<xsd:annotation>
					<xsd:documentation>Default</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="CT_Pitch">
		<xsd:annotation>
			<xsd:appinfo>
				<jaxb:class name="FontPitch"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:attribute name="val" type="ST_Pitch" use="required">
			<xsd:annotation>
				<xsd:documentation>Value</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="CT_FontSig">
		<xsd:annotation>
			<xsd:appinfo>
				<jaxb:class name="FontSig"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:attribute name="usb0" use="required" type="ST_LongHexNumber">
			<xsd:annotation>
				<xsd:documentation>First 32 Bits of Unicode Subset
					Bitfield</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="usb1" use="required" type="ST_LongHexNumber">
			<xsd:annotation>
				<xsd:documentation>Second 32 Bits of Unicode Subset
					Bitfield</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="usb2" use="required" type="ST_LongHexNumber">
			<xsd:annotation>
				<xsd:documentation>Third 32 Bits of Unicode Subset
					Bitfield</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="usb3" use="required" type="ST_LongHexNumber">
			<xsd:annotation>
				<xsd:documentation>Fourth 32 Bits of Unicode Subset
					Bitfield</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="csb0" use="required" type="ST_LongHexNumber">
			<xsd:annotation>
				<xsd:documentation>Lower 32 Bits of Code Page Bit
					Field</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="csb1" use="required" type="ST_LongHexNumber">
			<xsd:annotation>
				<xsd:documentation>Upper 32 Bits of Code Page Bit
					Field</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="CT_FontRel">
		<xsd:annotation>
			<xsd:appinfo>
				<jaxb:class name="FontRel"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="CT_Rel">
				<xsd:attribute name="fontKey" type="ST_Guid">
					<xsd:annotation>
						<xsd:documentation>Embedded Font Obfuscation
							Key</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="subsetted" type="xsd:boolean" default="true">
					<xsd:annotation>
						<xsd:documentation>Embedded Font Is
							Subsetted</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CT_DivBdr">
		<xsd:sequence>
			<xsd:element name="top" type="CT_Border" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Top Border for HTML div</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="left" type="CT_Border" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Left Border for HTML div</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="bottom" type="CT_Border" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Bottom Border for HTML
						div</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="right" type="CT_Border" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Right Border for HTML div</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_Div">
		<xsd:sequence>
			<xsd:element name="blockQuote" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Data for HTML blockquote
						Element</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="bodyDiv" type="BooleanDefaultTrue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Data for HTML body
						Element</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="marLeft" type="CT_SignedTwipsMeasure">
				<xsd:annotation>
					<xsd:documentation>Left Margin for HTML div</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="marRight" type="CT_SignedTwipsMeasure">
				<xsd:annotation>
					<xsd:documentation>Right Margin for HTML div</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="marTop" type="CT_SignedTwipsMeasure">
				<xsd:annotation>
					<xsd:documentation>Top Margin for HTML div</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="marBottom" type="CT_SignedTwipsMeasure">
				<xsd:annotation>
					<xsd:documentation>Bottom Margin for HTML
						div</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="divBdr" type="CT_DivBdr" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Set of Borders for HTML
						div</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="divsChild" type="CT_Divs" minOccurs="0"
				maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Child div Elements Contained within Current
						div</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="id" type="ST_DecimalNumber" use="required">
			<xsd:annotation>
				<xsd:documentation>div Data ID</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="CT_Divs">
		<xsd:sequence minOccurs="1" maxOccurs="unbounded">
			<xsd:element name="div" type="CT_Div">
				<xsd:annotation>
					<xsd:documentation>Information About Single HTML div
						Element</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_TxbxContent">
		<xsd:group ref="EG_BlockLevelElts" minOccurs="1" maxOccurs="unbounded"/>
	</xsd:complexType>
	<xsd:element name="txbxContent" type="CT_TxbxContent">
		<xsd:annotation>
			<xsd:documentation>Rich Text Box Content Container</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:group name="EG_MathContent">
		<xsd:choice>
			<xsd:element ref="m:oMathPara"></xsd:element>
			<xsd:element ref="m:oMath"></xsd:element>
		</xsd:choice>
	</xsd:group> 
	<xsd:group name="EG_BlockLevelChunkElts">
		<xsd:choice>
			<xsd:group ref="EG_ContentBlockContent" minOccurs="0"
				maxOccurs="unbounded"/>
		</xsd:choice>
	</xsd:group>
	<xsd:group name="EG_BlockLevelElts">
		<xsd:annotation>
			<xsd:appinfo>
				<jaxb:property name="Content"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:choice>
			<xsd:group ref="EG_BlockLevelChunkElts" minOccurs="0"
				maxOccurs="unbounded"/>
			<xsd:element name="altChunk" type="CT_AltChunk" minOccurs="0"
				maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Anchor for Imported External
						Content</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:group>


	<xsd:element name="ins">
		<xsd:annotation>
			<xsd:documentation>Inserted Run Content</xsd:documentation>
			<xsd:appinfo>
				<jaxb:class name="RunIns" />
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="CT_TrackChange">
					<xsd:choice minOccurs="0" maxOccurs="unbounded">
						<xsd:group ref="EG_ContentRunContent"></xsd:group>
						<xsd:group ref="m:EG_OMathMathElements" />
					</xsd:choice>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	
	<xsd:element name="del">
		<xsd:annotation>
			<xsd:documentation>Deleted Run Content</xsd:documentation>
			<xsd:appinfo>
				<jaxb:class name="RunDel" />
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="CT_TrackChange">
					<xsd:choice minOccurs="0" maxOccurs="unbounded">
						<xsd:group ref="EG_ContentRunContent"></xsd:group>
						<xsd:group ref="m:EG_OMathMathElements" />
					</xsd:choice>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	
	
	
	<xsd:group name="EG_RunLevelElts">
		<xsd:choice>
			<xsd:element ref="proofErr" minOccurs="0"/>
			<xsd:element name="permStart" minOccurs="0" type="CT_PermStart">
				<xsd:annotation>
					<xsd:documentation>Range Permission Start</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
