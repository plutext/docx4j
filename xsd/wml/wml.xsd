					<xsd:annotation>
						<xsd:documentation>Annotation Author</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="date" type="ST_DateTime" use="optional">
					<xsd:annotation>
						<xsd:documentation>Annotation Date</xsd:documentation>
