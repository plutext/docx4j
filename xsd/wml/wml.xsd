				<xsd:annotation>
					<xsd:documentation>Custom XML Element
						Properties</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:group ref="EG_ContentCellContent" minOccurs="0"
				maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="uri" type="ST_String">
			<xsd:annotation>
				<xsd:documentation>Custom XML Element Namespace</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="element" type="ST_String" use="required">
			<xsd:annotation>
				<xsd:documentation>Custom XML Element Name</xsd:documentation>
