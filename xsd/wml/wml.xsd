		<xsd:annotation>
			<xsd:documentation>Decimal Number Value</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:integer"/>
	</xsd:simpleType>
	<xsd:simpleType name="ST_UnsignedDecimalNumber">
