			<xsd:annotation>
				<xsd:documentation>Distance From Left of Table to
					Text</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="rightFromText" type="ST_TwipsMeasure">
			<xsd:annotation>
				<xsd:documentation>(Distance From Right of Table to
					Text</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="topFromText" type="ST_TwipsMeasure">
			<xsd:annotation>
				<xsd:documentation>Distance From Top of Table to
					Text</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="bottomFromText" type="ST_TwipsMeasure">
			<xsd:annotation>
				<xsd:documentation>Distance From Bottom of Table to
					Text</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="vertAnchor" type="ST_VAnchor">
			<xsd:annotation>
				<xsd:documentation>Table Vertical Anchor</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="horzAnchor" type="ST_HAnchor">
			<xsd:annotation>
				<xsd:documentation>Table Horizontal Anchor</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="tblpXSpec" type="ST_XAlign">
			<xsd:annotation>
				<xsd:documentation>Relative Horizontal Alignment From
					Anchor</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="tblpX" type="ST_SignedTwipsMeasure">
			<xsd:annotation>
				<xsd:documentation>Absolute Horizontal Distance From
					Anchor</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="tblpYSpec" type="ST_YAlign">
			<xsd:annotation>
				<xsd:documentation>Relative Vertical Alignment from
					Anchor</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="tblpY" type="ST_SignedTwipsMeasure">
			<xsd:annotation>
				<xsd:documentation>Absolute Vertical Distance From
					Anchor</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
