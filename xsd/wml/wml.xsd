	<xsd:group name="EG_ContentRunContent">
		<xsd:choice>
			<xsd:element name="customXml" type="CT_CustomXmlRun">
				<xsd:annotation>
					<xsd:documentation>Inline-Level Custom XML
						Element</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="smartTag" type="CT_SmartTagRun">
				<xsd:annotation>
					<xsd:documentation>Inline-Level Smart Tag</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="sdt" type="CT_SdtRun">
				<xsd:annotation>
					<xsd:documentation>Inline-Level Structured Document
						Tag</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- from ECMA 3376 4ed transitional -->
			<xsd:element name="dir">
				<xsd:complexType>
					<xsd:group ref="EG_PContent" minOccurs="0" maxOccurs="unbounded" />
					<xsd:attribute name="val" type="ST_Direction" use="optional" />
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="bdo">
				<xsd:complexType>
					<xsd:group ref="EG_PContent" minOccurs="0" maxOccurs="unbounded" />
					<xsd:attribute name="val" type="ST_Direction" use="optional" />
				</xsd:complexType>
			</xsd:element>	
			<!-- end addition from ECMA 3376 4ed transitional -->
			
			<xsd:element ref="r"/>
			<xsd:group ref="EG_RunLevelElts" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:choice>
	</xsd:group>

  <xsd:simpleType name="ST_Direction">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="ltr"/>
      <xsd:enumeration value="rtl"/>
