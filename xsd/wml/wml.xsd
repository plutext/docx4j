				<xsd:annotation>
					<xsd:documentation>Table Indent from Leading
						Margin</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="tblBorders" type="CT_TblBorders" minOccurs="0"
