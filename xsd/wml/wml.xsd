		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CT_TcPrChange">
		<xsd:complexContent>
			<xsd:extension base="CT_TrackChange">
				<xsd:sequence>
					<xsd:element name="tcPr" type="CT_TcPrInner" minOccurs="1">
