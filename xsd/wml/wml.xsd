			<xsd:length value="3" fixed="true"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ST_HexColor">
		<xsd:annotation>
			<xsd:documentation>Color Value</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ST_HexColorAuto ST_HexColorRGB"/>
	</xsd:simpleType>
